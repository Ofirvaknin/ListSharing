<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LoggedIn.AddUserController">
   <right>
      <AnchorPane prefHeight="500.0" prefWidth="415.0">
         <children>
            <Label fx:id="lblHello" layoutX="84.0" layoutY="41.0" prefHeight="30.0" prefWidth="257.0" text="Choose a list to add user to" textFill="#0598ff">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Button fx:id="btnAdd" layoutX="137.0" layoutY="166.0" mnemonicParsing="false" onMousePressed="#handleButtonAdd" prefHeight="25.0" prefWidth="122.0" style="-fx-background-color: #0598ff;" text="Add" textFill="WHITE" />
            <Button fx:id="btnReturn" layoutX="137.0" layoutY="200.0" mnemonicParsing="false" onMousePressed="#handleButtonReturn" prefHeight="25.0" prefWidth="122.0" style="-fx-background-color: #0598ff;" text="Return" textFill="WHITE" />
            <Label fx:id="lblError" layoutX="132.0" layoutY="141.0" prefHeight="17.0" prefWidth="255.0" />
            <ComboBox fx:id="comboBoxLists" layoutX="131.0" layoutY="71.0" onMousePressed="#setComboBox" prefWidth="150.0" promptText="Choose List" style="-fx-background-color: transparent; -fx-border-color: #0598ff; -fx-border-width: 0px 0px 2px 0px;" />
            <TextField fx:id="txtfldUserName" layoutX="132.0" layoutY="103.0" promptText="User name" style="-fx-background-color: transparent; -fx-border-color: #0598ff; -fx-border-width: 0px 0px 2px 0px;" />
            <FontAwesomeIconView fill="#0598ff" glyphName="USER_PLUS" layoutX="119.0" layoutY="184.0" size="15" />
            <FontAwesomeIconView fill="#0598ff" glyphName="UNDO" layoutX="119.0" layoutY="219.0" size="15" />
         </children>
      </AnchorPane>
   </right>
   <left>
      <AnchorPane prefHeight="500.0" prefWidth="300.0" style="-fx-background-color: #0598ff;" BorderPane.alignment="CENTER">
         <children>
            <FontAwesomeIconView fill="WHITE" glyphName="LIST" layoutX="100.0" layoutY="219.0" size="100" />
            <Text fill="WHITE" layoutX="14.0" layoutY="270.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Shared List Managment">
               <font>
                  <Font size="25.0" />
               </font>
            </Text>
         </children></AnchorPane>
   </left>
</BorderPane>
