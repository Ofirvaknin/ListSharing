<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<BorderPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LoggedIn.RemoveListController">
   <right>
      <AnchorPane prefHeight="500.0" prefWidth="415.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="lblAddProd" layoutX="75.0" layoutY="32.0" prefHeight="30.0" prefWidth="150.0" text="Remove List" textFill="#0598ff">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
            <Button fx:id="btnRemoveList" layoutX="75.0" layoutY="127.0" mnemonicParsing="false" onMousePressed="#handleButtonRemoveList" prefHeight="25.0" prefWidth="96.0" style="-fx-background-color: #0598ff;" text="Remove" textFill="WHITE" />
            <ComboBox fx:id="comboBoxList" layoutX="75.0" layoutY="80.0" onMousePressed="#setComboBoxList" prefWidth="150.0" promptText="Choose List" style="-fx-background-color: transparent; -fx-border-width: 0px 0px 2px 0px; -fx-border-color: #0598ff;" />
            <Button fx:id="btnReturn" layoutX="76.0" layoutY="163.0" mnemonicParsing="false" onMousePressed="#handleButtonReturn" prefHeight="19.0" prefWidth="95.0" style="-fx-background-color: #0598ff;" text="Return" textFill="WHITE" />
            <Label fx:id="lblError" layoutX="234.0" layoutY="87.0" prefHeight="17.0" prefWidth="170.0" />
            <TextArea layoutX="191.0" layoutY="124.0" prefHeight="67.0" prefWidth="201.0" text="Note: You can only delete lists&#10;that you have created.&#10;&#10;" />
            <FontAwesomeIconView fill="#0598ff" glyphName="REMOVE" layoutX="54.0" layoutY="144.0" size="15" />
            <FontAwesomeIconView fill="#0598ff" glyphName="UNDO" layoutX="54.0" layoutY="180.0" size="15" />
         </children>
      </AnchorPane>
   </right>
   <left>
      <AnchorPane prefHeight="500.0" prefWidth="315.0" style="-fx-background-color: #0598ff;" BorderPane.alignment="CENTER">
         <children>
            <FontAwesomeIconView fill="WHITE" glyphName="LIST" layoutX="100.0" layoutY="219.0" size="100" />
            <Text fill="WHITE" layoutX="14.0" layoutY="270.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Shared List Managment">
               <font>
                  <Font size="25.0" />
               </font>
            </Text>
         </children>
      </AnchorPane>
   </left>
</BorderPane>
